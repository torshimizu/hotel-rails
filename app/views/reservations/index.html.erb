<%# I want this to check the availability of a room %>
<%# Therefore I need to have an input field for start and end date %>

<%= form_tag admin_index_path, method: 'get', class: 'check_availability' do %> 
  <%= label_tag :start_date %>
  <%= date_field_tag :start_date %>
  <br>
  <br>
  <%= label_tag :end_date %>
  <%= date_field_tag :end_date %>
<% end %>
