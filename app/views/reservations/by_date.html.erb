<% if !@days_reservations.nil? %>
  <% @days_reservations.each do |reservation| %>
  <%= render partial: 'shared/reservation-list', locals: { var: reservation } %>
  <% end %>
<% end %>


<% if !@found_reservations.nil? %>
  <ul>
    <% @found_reservations.each do |reservation| %>
    <%= render partial: 'shared/reservation-list', locals: { var: reservation } %>
    <% end %>
  </ul>
<% end %>
